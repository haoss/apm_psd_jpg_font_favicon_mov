doctype

html(lang="ru")
  
  -var active = 'Sale car' //активный пункт меню, список в header.jade
  -var pageTitle = 'Sale car' + ' - APM Group' //title страницы для генерации верстки  

  include ./_include/_head

  body.body
    // Preloader
    include ./_include/_preloader
    
    .base
      include ./_include/_header
      
      // Content
      .content
        .container
          
          .section__head
            h1 2017 Lexus LS Sedan
            
          .car-product
            .row
              .car-product__left.col-md-8
                
                // Tab panes
                .car-product__carousel-top
                  #gallery1.tab-pane.active(role='tabpanel')
                    img(src="./img/cars-list/car-carousel-top-1.png", alt="").img-responsive
                  #gallery2.tab-pane(role='tabpanel')
                    img(src="./img/cars-list/car-carousel-top-2.png", alt="").img-responsive
                  #gallery3.tab-pane(role='tabpanel')
                    img(src="./img/cars-list/car-carousel-top-3.png", alt="").img-responsive
                
                // Nav tabs
                ul.car-product__carousel-bottom(role='tablist')
                  li.active(role='presentation')
                    a(href='#gallery1', aria-controls='gallery1', role='tab', data-toggle='tab')
                      img(src="./img/cars-list/car-carousel-bottom-1.png", alt="")
                  li(role='presentation')
                    a(href='#gallery2', aria-controls='gallery2', role='tab', data-toggle='tab')
                      img(src="./img/cars-list/car-carousel-bottom-2.png", alt="")
                  li(role='presentation')
                    a(href='#gallery3', aria-controls='gallery3', role='tab', data-toggle='tab')
                      img(src="./img/cars-list/car-carousel-bottom-3.png", alt="")
                    
                
              .car-product__right.col-md-4
                
                .car-product__title The Basics
                
                .car-product__th
                  table
                    tr
                      td
                        img(src="data:image/png;base64,", alt="")
                      td 8 speed Automatic | CVT Automatic
                    tr
                      td
                        img(src="data:image/png;base64,", alt="")
                      td Rear Wheel Drive | Four Wheel Drive
                    tr
                      td
                        img(src="data:image/png;base64,", alt="")
                      td 8.6 - 11.1 L/100km
                    tr
                      td
                        img(src="data:image/png;base64,", alt="")
                      td Petrol
                    tr
                      td
                        img(src="data:image/png;base64,", alt="")
                      td 5 Passengers
                    tr
                      td
                        img(src="data:image/png;base64,", alt="")
                      td 4 Doors
                
                .car-product__price
                  div
                    span $ 
                    | 213 999
                a(href="#!").btn.btn--blue.car-product__btn
                  span Request a call
          
          .text-center
            p Get the benefit of lane departure warning. 10 airbags to protect you and your family. It has a 4.6 litre engine and 285kW of power!. This Stunning New Lexus LS460 2017 has voice recognition. This car has keyless start, driver airbag, woodgrain - dash board, front & rear power windows, bluetooth functionality, power tailgate/boot and hill holder. Collision avoidance system. Inbuilt GPS navigation system. Has blind spot sensors. Rear vision camera so you can easily see what is behind you when reversing. This car has parking assist graphical display. It has storage compartment in centre console. This car has active cornering headlamps, information display, brake assist, digital radio (DAB+), adaptive cruise control, HID headlamps, rear cup holders and premium sound system. 
          
          .clear30
          
          .section__head.section__head--line
            h2 you will like it
      
          .cars-list.cars-list--carousel
            .cars-list__wrapper
              a(href="#!").cars-list__block
                .cars-list__img
                  img(src="./img/cars-list/cars-list-carousel-1.png")
                span.cars-list__info 2017
                span.cars-list__title Lexus CT Hatch
                span.cars-list__info From 
                  strong $46,291 
                  | Drive away
              a(href="#!").cars-list__block
                .cars-list__img
                  img(src="./img/cars-list/cars-list-carousel-2.png")
                span.cars-list__info 2017
                span.cars-list__title Lexus IS Sedan
                span.cars-list__info From 
                  strong $66,535 
                  | Drive away
              a(href="#!").cars-list__block
                .cars-list__img
                  img(src="./img/cars-list/cars-list-carousel-3.png")
                span.cars-list__info 2017
                span.cars-list__title Lexus LS Sedan
                span.cars-list__info From 
                  strong $201,962 
                  | Drive away
      
      include ./_include/_footer
      //- div(style="height: 1000px; background-color: #000")
    
    //[if gte IE 9]
      <script src='./js/libs/html5shiv/es5-shim.min.js'>
      <script src='./js/libs/html5shiv/html5shiv.min.js'>
      <script src='./js/libs/html5shiv/html5shiv-printshiv.min.js'>
      <script src='./js/libs/html5shiv/respond.min.js'><![endif]

    // Load Scripts
    script(src="./js/libs.js")
    include ./_include/_footer-js
    script(src="./js/common.js")
    include ./_include/_js-body-bottom
